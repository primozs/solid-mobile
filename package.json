{
  "name": "solid-mobile",
  "version": "0.0.0",
  "description": "solid pwa capacitor ionic",
  "author": "Primoz Susa <primoz.susa@pm.me>",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development vite",
    "build": "NODE_ENV=production yarn lint && yarn type-check && vite build",
    "preview": "NODE_ENV=production vite preview --port 3001",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore",
    "test": "vitest",
    "cap": "cap",
    "cap-build": "yarn build && cap copy && cap sync",
    "cap-open-android": "cap open android",
    "cap-run-android": "cap run android",
    "ionic-serve": "ionic serve --external",
    "ionic-live-reload": "ionic cap run android --livereload-url=http://192.168.102.116:8100",
    "ionic:build": "yarn build",
    "ionic:serve": "yarn dev",
    "cap-add-android": "cap add android",
    "cordova-res-android": "cordova-res android --skip-config --copy"
  },
  "license": "MIT",
  "dependencies": {
    "@capacitor/android": "^4.6.0",
    "@capacitor/app": "^4.1.1",
    "@capacitor/core": "^4.6.0",
    "@capacitor/haptics": "^4.1.0",
    "@capacitor/ios": "^4.6.0",
    "@capacitor/keyboard": "^4.1.0",
    "@capacitor/preferences": "^4.0.2",
    "@capacitor/status-bar": "^4.1.1",
    "@ionic/core": "^6.3.9",
    "@ionic/pwa-elements": "^3.1.1",
    "@solid-primitives/refs": "^0.3.4",
    "@solidjs/router": "^0.5.1",
    "ionicons": "^6.0.4",
    "solid-js": "^1.3.12"
  },
  "devDependencies": {
    "@capacitor/cli": "^4.6.0",
    "@ionic/cli": "^6.20.4",
    "@solidjs/testing-library": "^0.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "cordova-res": "^0.15.4",
    "eslint": "^8.28.0",
    "eslint-plugin-solid": "^0.9.1",
    "jsdom": "^20.0.3",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "typescript": "^4.6.2",
    "vite": "^3.2.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.13.3",
    "vite-plugin-solid": "^2.2.6",
    "vitest": "^0.25.3",
    "workbox-core": "^6.5.4",
    "workbox-precaching": "^6.5.4"
  }
}
